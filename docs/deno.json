{
  "tasks": {
    "lume": "echo \"import 'lume/cli.ts'\" | deno run -A -",
    "build": "deno task lume",
    "serve": "deno task lume -s"
  },
  "imports": {
    "lume/": "https://deno.land/x/lume@v2.0.0/",
    "@tailwindcss/typography": "npm:@tailwindcss/typography@^0.5.10"
  },
  "compilerOptions": {
    "types": [
      "lume/types.ts"
    ]
  }
}
